{
  "dataset_reader": {
    "name": "basic_classification_reader",
    "x": "text",
    "y": "intents",
    "data_path": "snips"
  },
  "dataset_iterator": {
    "name": "basic_classification_iterator",
    "seed": 42,
    "field_to_split": "train",
    "split_fields": [
      "train",
      "valid"
    ],
    "split_proportions": [
      0.9,
      0.1
    ]
  },
  "chainer": {
    "in": [
      "x"
    ],
    "in_y": [
      "y"
    ],
    "pipe": [
      [
        {
          "id": "classes_vocab",
          "component_name": "Vocab of classes",
          "name": "default_vocab",
          "fit_on": [
            "y"
          ],
          "level": "token",
          "save_path": "vocabs/snips_classes.dict",
          "load_path": "vocabs/snips_classes.dict"
        }
      ],
      [
        {
          "component_name": "Convert to lowercase",
          "in": ["x"],
          "name": "str_lower",
          "out": ["x"]
        },
        null
      ],
      [
        {
          "component_name": "NLTK tokenizer",
          "in": "x",
          "out": "x_tok",
          "id": "my_tokenizer",
          "name": "nltk_tokenizer",
          "tokenizer": "wordpunct_tokenize"
        },
        {
          "component_name": "Lazy tokenizer",
          "in": "x",
          "out": "x_tok",
          "id": "my_tokenizer",
          "name": "lazy_tokenizer",
          "tokenizer": "wordpunct_tokenize"
        }
      ],
      [
        {
          "component_name": "dstc2_fasttext_100",
          "in": "x_tok",
          "out": "x_vec",
          "id": "fasttext_embedder",
          "name": "fasttext",
          "save_path": "embeddings/dstc2_fastText_model.bin",
          "load_path": "embeddings/dstc2_fastText_model.bin",
          "dim": 100
        }
      ],
      [
        {
          "in": [
            "x_vec"
          ],
          "in_y": [
            "y"
          ],
          "out": [
            "y_labels",
            "y_probas_dict"
          ],
          "main": true,
          "name": "keras_classification_model",
          "component_name": "WCNN",
          "save_path": "intents/intent_cnn_snips_v4",
          "classes": "#classes_vocab.keys()",
          "kernel_sizes_cnn": [
            1,
            2,
            3
          ],
          "filters_cnn": 256,
          "confident_threshold": 0.5,
          "optimizer": "Adam",
          "lear_rate": 0.01,
          "lear_rate_decay": 0.1,
          "loss": "binary_crossentropy",
          "text_size": 15,
          "coef_reg_cnn": 1e-4,
          "coef_reg_den": 1e-4,
          "dropout_rate": 0.5,
          "dense_size": 100,
          "model_name": "cnn_model",
          "embedding_size": 100
        },
        {
          "in": [
            "x_vec"
          ],
          "in_y": [
            "y"
          ],
          "out": [
            "y_labels",
            "y_probas_dict"
          ],
          "main": true,
          "name": "keras_classification_model",
          "component_name": "Bi-LSTM",
          "save_path": "intents/intent_cnn_snips_v4",
          "classes": "#classes_vocab.keys()",
          "units_lstm": 128,
          "rec_dropout_rate": 0.5,
          "confident_threshold": 0.5,
          "optimizer": "Adam",
          "lear_rate": 0.01,
          "lear_rate_decay": 0.1,
          "loss": "binary_crossentropy",
          "text_size": 15,
          "coef_reg_lstm": 1e-4,
          "coef_reg_den": 1e-4,
          "dropout_rate": 0.5,
          "dense_size": 100,
          "model_name": "bilstm_model",
          "embedding_size": 100
        }
      ]
    ],
    "out": [
      "y_labels",
      "y_probas_dict"
    ]
  },
  "train": {
    "epochs": 10,
    "batch_size": 64,
    "metrics": [
      "classification_accuracy",
      "classification_f1",
      "classification_f1_weighted"
    ],
    "validation_patience": 5,
    "val_every_n_epochs": 5,
    "log_every_n_epochs": 5,
    "show_examples": false,
    "validate_best": true,
    "test_best": false
  },
  "metadata": {
    "requirements": [
      "../dp_requirements/tf.txt",
      "../dp_requirements/fasttext.txt"
    ],
    "labels": {
      "telegram_utils": "IntentModel",
      "server_utils": "KerasIntentModel"
    },
    "download": [
      "http://files.deeppavlov.ai/deeppavlov_data/intents.tar.gz",
      "http://files.deeppavlov.ai/deeppavlov_data/vocabs.tar.gz",
      {
        "url": "http://files.deeppavlov.ai/datasets/snips_intents/train.csv",
        "subdir": "snips"
      },
      {
        "url": "http://files.deeppavlov.ai/deeppavlov_data/embeddings/dstc2_fastText_model.bin",
        "subdir": "embeddings"
      }
    ]
  }
}
