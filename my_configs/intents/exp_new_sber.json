{
  "dataset_reader": {
    "name": "basic_classification_reader",
    "x": "text",
    "y": "target",
    "data_path": "new_sber"
  },
  "dataset_iterator": {
    "name": "basic_classification_iterator",
    "seed": 42,
    "field_to_split": "train",
    "split_fields": [
      "train",
      "valid"
    ],
    "split_proportions": [
      0.9,
      0.1
    ]
  },
  "chainer": {
    "in": [
      "x"
    ],
    "in_y": [
      "y"
    ],
    "search": true,
    "pipe": [
      [
        {
        "id": "classes_vocab",
        "name": "default_vocab",
        "fit_on": [
          "y"
        ],
        "level": "token",
        "save_path": "vocabs/sber_classes.dict",
        "load_path": "vocabs/sber_classes.dict"
      }
      ],
      [
        {
        "id": "my_embedder",
        "name": "fasttext",
        "save_path": "embeddings/ft_native_300_ru_wiki_lenta_nltk_word_tokenize.bin",
        "load_path": "embeddings/ft_native_300_ru_wiki_lenta_nltk_word_tokenize.bin",
        "dim": 300
        },
        {
          "id": "my_embedder",
          "name": "glove",
          "save_path": "embeddings/new_sber_w2v.txt",
          "load_path": "embeddings/new_sber_w2v.txt",
          "dim": 100
        }
      ],
      [
        {
          "id": "my_tokenizer",
          "name": "nltk_tokenizer",
          "tokenizer": "wordpunct_tokenize"
        }
      ],
      [
        {
        "in": ["x"],
        "in_y": ["y"],
        "out": [
          "y_labels",
          "y_probas_dict"
        ],
        "main": true,
        "search": true,
        "name": "keras_classification_model",
        "save_path": "new_sber/checkpoints/cnn",
        "classes": "#classes_vocab.keys()",
        "kernel_sizes_cnn": [[
          1,
          2,
          3
        ]],
        "filters_cnn": [128, 256, 512],
        "confident_threshold": 1.0,
        "optimizer": "Adam",
        "lear_rate": [0.0001, 0.001, 0.01],
        "lear_rate_decay": [0.0, 0.1],
        "loss": "categorical_crossentropy",
        "text_size": 50,
        "coef_reg_cnn": [0.0, 1e-4, 1e-3],
        "coef_reg_den": [0.0, 1e-4, 1e-3],
        "dropout_rate": [0.0, 0.3, 0.5],
        "dense_size": [100, 150],
        "model_name": "cnn_model",
        "embedder": "#my_embedder",
        "tokenizer": "#my_tokenizer"
      },
        {
        "in": [
          "x"
        ],
        "in_y": [
          "y"
        ],
        "out": [
          "y_labels",
          "y_probas_dict"
        ],
        "main": true,
        "search": true,
        "name": "keras_classification_model",
        "save_path": "new_sber/checkpoints/bilstm",
        "classes": "#classes_vocab.keys()",
        "units_lstm": [64, 128, 256],
        "rec_dropout_rate": [0.0, 0.3 ,0.5],
        "confident_threshold": 1.0,
        "optimizer": "Adam",
        "lear_rate": [0.0001, 0.001, 0.01],
        "lear_rate_decay": [0.0, 0.1],
        "loss": "categorical_crossentropy",
        "text_size": 50,
        "coef_reg_lstm": [0.0, 1e-4, 1e-3],
        "coef_reg_den": [0.0, 1e-4, 1e-3],
        "dropout_rate": [0.0, 0.3 ,0.5],
        "dense_size": [100, 150],
        "model_name": "bilstm_model",
        "embedder": "#my_embedder",
        "tokenizer": "#my_tokenizer"
      }
      ]
    ],
    "out": [
      "y_labels",
      "y_probas_dict"
    ]
  },
  "train": {
    "epochs": 50,
    "batch_size": 256,
    "metrics": [
      "classification_accuracy",
      "classification_f1",
      "classification_f1_weighted"
    ],
    "validation_patience": 5,
    "val_every_n_epochs": 20,
    "log_every_n_epochs": 5,
    "show_examples": false,
    "validate_best": true,
    "test_best": true
  },
  "metadata": {
    "requirements": [
      "../dp_requirements/tf.txt",
      "../dp_requirements/fasttext.txt",
      "../dp_requirements/gensim.txt"
    ],
    "labels": {
      "telegram_utils": "IntentModel",
      "server_utils": "KerasIntentModel"
    }
  }
}
