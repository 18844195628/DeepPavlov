{
  "chainer": {
    "in": [
      "question_raw"
    ],
    "out": [
      "rank_text_score_id"
    ],
    "pipe": [
      {
        "name": "wiki_sqlite_vocab",
        "in": [
          "question_raw"
        ],
        "out": [
          "context_raw",
          "doc_ids"
        ],
        "data_dir": "pmef",
        "shuffle": false,
        "join_docs": false,
        "return_all_content": true,
        "load_path": "http://lnsigo.mipt.ru/export/datasets/pmef/en_drones.db"
      },
      {
        "name": "query_paragraph_tuplifier",
        "in": [
          "question_raw",
          "context_raw",
          "doc_ids"
        ],
        "out": [
          "question_paragraph_tuples"
        ]
      },
      {
        "name": "squad_paragraph_ranker",
        "top_n": 10,
        "ranker_config": "../deeppavlov/configs/squad/squad_selqa_noans.json",
        "type": "paragraph",
        "in": [
          "question_paragraph_tuples"
        ],
        "out": ["docs", "doc_scores", "doc_ids"]
      },
      {
        "name": "rank_doc_score_tuplifier",
        "in": [
          "docs",
          "doc_scores",
          "doc_ids"
        ],
        "out": [
          "rank_text_score_id"
        ]
      }
    ]
  },
  "train": {
    "validate_best": false,
    "test_best": false,
    "batch_size": 10000
  },
  "metadata": {
    "download": [
      "http://lnsigo.mipt.ru/export/deeppavlov_data/pmef.tar.gz",
      {
        "url": "http://lnsigo.mipt.ru/export/datasets/pmef/en_drones.db",
        "subdir": "pmef"
      },
      "http://lnsigo.mipt.ru/export/deeppavlov_data/squad_selqa_model_noans.tar.gz",
      {
        "url": "http://lnsigo.mipt.ru/export/embeddings/wiki-news-300d-1M.vec",
        "subdir": "embeddings"
      },
      {
        "url": "http://lnsigo.mipt.ru/export/embeddings/wiki-news-300d-1M-char.vec",
        "subdir": "embeddings"
      }
    ]
  }
}