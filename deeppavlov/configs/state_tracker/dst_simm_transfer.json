{
  "dataset_reader": {
    "class_name": "sim_m_reader",
    "mode": "train_on_valid",
    "data_path": "{DOWNLOADS_PATH}/google-dialogues"
  },
  "dataset_iterator": {
    "class_name": "dialog_state_iterator"
  },
  "chainer": {
    "in": [
        "u_utter", "u_slots_bio", "u_acts", "s_tokens", "s_slots_bio", "s_acts",
        "prev_state"
    ],
    "in_y": ["state"],
    "out": [],
    "pipe": [
      {
        "class_name": "simple_vocab",
        "save_path": "{MODELS_PATH}/dst/slot_names_simm.dict",
        "fit_on": ["state"]
      },
      {
        "class_name": "lazy_tokenizer",
        "in": ["u_utter"],
        "out": ["u_tokens"]
      },
      {
        "class_name": "deeppavlov.models.state_tracker.preprocessors:Delexicalizor",
        "in": ["u_tokens", "u_slots_bio"],
        "out": ["u_norm_tokens"]
      },
      {
        "class_name": "deeppavlov.models.state_tracker.preprocessors:Delexicalizor",
        "in": ["s_tokens", "s_slots_bio"],
        "out": ["s_norm_tokens"]
      },
      {
        "class_name": "simple_vocab",
        "save_path": "{MODELS_PATH}/dst/word_simm.dict",
        "load_path": "{MODELS_PATH}/dst/word_simm.dict",
        "fit_on": ["u_norm_tokens", "s_norm_tokens"],
        "special_tokens": ["<unk>"]
      }
    ]
  },
  "train": {
    "validate_best": false,
    "test_best": false
  },
  "metadata": {
     "variables": {
       "ROOT_PATH": "~/.deeppavlov",
       "DOWNLOADS_PATH": "{ROOT_PATH}/downloads",
       "MODELS_PATH": "{ROOT_PATH}/models"
    },
    "requirements": [],
    "labels": {},
    "download": []
  }
}
