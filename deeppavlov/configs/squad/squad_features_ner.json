{
  "dataset_reader": {
    "name": "squad_dataset_reader",
    "data_path": "squad/",
    "dataset": "NERSQuAD"
  },
  "dataset_iterator": {
    "name": "ner_squad_iterator",
    "seed": 1337,
    "shuffle": true
  },
  "chainer": {
    "in": ["context_raw", "question_raw"],
    "in_y": ["ans_raw", "ans_raw_start"],
    "pipe": [
      {
        "name": "squad_preprocessor",
        "id": "squad_prepr",
        "context_limit": 450,
        "question_limit": 150,
        "char_limit": 16,
        "in": ["context_raw", "question_raw"],
        "out": ["context", "context_tokens", "context_chars",
                "c_r2p", "c_p2r", "question",
                "question_tokens", "question_chars", "spans"]
      },
      {
        "name": "squad_ans_preprocessor",
        "id": "squad_ans_prepr",
        "in": ["ans_raw", "ans_raw_start", "c_r2p", "spans"],
        "out": ["ans", "ans_start", "ans_end"]
      },
      {
        "name": "squad_vocab_embedder",
        "id": "vocab_embedder",
        "level": "token",
        "emb_folder": "embeddings/",
        "emb_url": "http://lnsigo.mipt.ru/export/embeddings/wiki-news-300d-1M.vec",
        "save_path": "squad_model_features_ner/emb/vocab_embedder.pckl",
        "load_path": "squad_model_features_ner/emb/vocab_embedder.pckl",
        "context_limit": "#squad_prepr.context_limit",
        "question_limit": "#squad_prepr.question_limit",
        "char_limit": "#squad_prepr.char_limit",
        "fit_on": ["context_tokens", "question_tokens"],
        "in": ["context_tokens", "question_tokens"],
        "out": ["context_tokens_idxs", "question_tokens_idxs"]
      },
      {
        "name": "squad_vocab_embedder",
        "id": "char_vocab_embedder",
        "level": "char",
        "emb_folder": "embeddings/",
        "emb_url": "http://lnsigo.mipt.ru/export/embeddings/wiki-news-300d-1M-char.vec",
        "save_path": "squad_model_features_ner/emb/char_vocab_embedder.pckl",
        "load_path": "squad_model_features_ner/emb/char_vocab_embedder.pckl",
        "context_limit": "#squad_prepr.context_limit",
        "question_limit": "#squad_prepr.question_limit",
        "char_limit": "#squad_prepr.char_limit",
        "fit_on": ["context_chars", "question_chars"],
        "in": ["context_chars", "question_chars"],
        "out": ["context_chars_idxs", "question_chars_idxs"]
      },
      {
        "name": "squad_ner_tags_extractor",
        "id": "ner_extractor",
        "ner_config": "../deeppavlov/configs/ner/ner_ontonotes.json",
        "in": ["context_tokens"],
        "out": ["context_ner_tags"]
      },
      {
        "ref": "ner_extractor",
        "in": ["question_tokens"],
        "out": ["question_ner_tags"]
      },
      {
        "name": "squad_ner_tags_preprocessor",
        "tokens_limit": "#squad_prepr.context_limit",
        "in": ["context_ner_tags"],
        "out": ["context_ner_tags"]
      },
      {
        "name": "squad_ner_tags_preprocessor",
        "tokens_limit": "#squad_prepr.question_limit",
        "in": ["question_ner_tags"],
        "out": ["question_ner_tags"]
      },
      {
        "in": ["context_ner_tags"],
        "id": "tag_vocab",
        "name": "simple_vocab",
        "pad_with_zeros": false,
        "unk_token": "<UNK>",
        "fit_on": ["context_ner_tags"],
        "save_path": "squad_model_features_ner/ner_tags.dict",
        "load_path": "squad_model_features_ner/ner_tags.dict",
        "out": ["context_ner_tags_idxs"]
      },
      {
        "ref": "tag_vocab",
        "in": ["question_ner_tags"],
        "out": ["question_ner_tags_idxs"]
      },
      {
        "name": "squad_features_extractor",
        "context_limit": "#squad_prepr.context_limit",
        "question_limit": "#squad_prepr.question_limit",
        "in": ["context_tokens", "question_tokens"],
        "out": ["context_features", "question_features"]
      },
      {
        "name": "squad_model",
        "id": "squad",
        "word_emb": "#vocab_embedder.emb_mat",
        "char_emb": "#char_vocab_embedder.emb_mat",
        "context_limit": "#squad_prepr.context_limit",
        "question_limit": "#squad_prepr.question_limit",
        "char_limit": "#squad_prepr.char_limit",
        "train_char_emb": true,
        "use_features": true,
        "use_ner_features": true,
        "ner_vocab_size": "#tag_vocab.len",
        "char_hidden_size": 100,
        "encoder_hidden_size": 75,
        "attention_hidden_size": 75,
        "learning_rate": 0.5,
        "min_learning_rate": 0.001,
        "learning_rate_patience": 2,
        "keep_prob": 0.7,
        "grad_clip": 5.0,
        "weight_decay": 1.0,
        "legacy": false,
        "save_path": "squad_model_features_ner/model",
        "load_path": "squad_model_features_ner/model",
        "in": {
          "c_tokens": "context_tokens_idxs",
          "c_chars": "context_chars_idxs",
          "q_tokens": "question_tokens_idxs",
          "q_chars": "question_chars_idxs",
          "c_str": "context_tokens",
          "q_str": "question_tokens",
          "c_features": "context_features",
          "q_features": "question_features",
          "c_ner": "context_ner_tags_idxs",
          "q_ner": "question_ner_tags_idxs"
        },
        "in_y": {
          "y1s": "ans_start",
          "y2s": "ans_end"
        },
        "out": ["ans_start_predicted", "ans_end_predicted", "prob", "logit"]
      },
      {
        "name": "squad_ans_postprocessor",
        "id": "squad_ans_postprepr",
        "in": ["ans_start_predicted", "ans_end_predicted", "context_raw", "c_p2r", "spans"],
        "out": ["ans_predicted", "ans_start_predicted", "ans_end_predicted"]
      }
    ],
    "out": ["ans_predicted", "ans_start_predicted", "prob", "logit"]
  },
  "train": {
    "show_examples": false,
    "test_best": false,
    "validate_best": true,
    "log_every_n_batches": 250,
    "val_every_n_epochs": 1,
    "batch_size": 50,
    "pytest_max_batches": 2,
    "validation_patience": 10,
    "metrics": ["squad_f1", "exact_match"]
  },
  "metadata": {
    "requirements": [
      "../dp_requirements/tf-gpu.txt"
    ],
    "labels": {
      "telegram_utils": "SquadModel",
      "server_utils": "SquadModel"
    },
    "download": [
      "http://TODO",
      "http://files.deeppavlov.ai/deeppavlov_data/ner_ontonotes_v3_cpu_compatible.tar.gz",
      {
        "url": "http://files.deeppavlov.ai/embeddings/glove.6B.100d.txt",
        "subdir": "embeddings"
      },
      {
        "url": "http://lnsigo.mipt.ru/export/embeddings/wiki-news-300d-1M.vec",
        "subdir": "embeddings"
      },
      {
        "url": "http://lnsigo.mipt.ru/export/embeddings/wiki-news-300d-1M-char.vec",
        "subdir": "embeddings"
      }
      ]
  }
}

