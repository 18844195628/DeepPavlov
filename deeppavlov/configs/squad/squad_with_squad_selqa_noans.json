{
 "dataset_reader": {
    "name": "squad_dataset_reader",
    "dataset": "MultiSQuAD",
    "data_path": "multi_squad/"
  },
  "dataset_iterator": {
    "name": "multi_squad_iterator",
    "seed": 1337,
    "shuffle": true,
    "with_answer_rate": 0.666
  },
  "chainer": {
    "in": ["context_raw", "question_raw"],
    "in_y": ["ans_raw", "ans_raw_start"],
    "out": ["ans_predicted", "ans_start_predicted", "score"],
    "pipe": [
      {
        "config_path": "../deeppavlov/configs/squad/squad.json",
        "in": ["context_raw", "question_raw"],
        "out": ["ans_predicted", "ans_start_predicted", "prob"]
      },
      {
        "config_path": "../deeppavlov/configs/squad/squad_selqa_noans.json",
        "in": ["context_raw", "question_raw"],
        "out": ["ans_predicted_1", "ans_start_predicted_1", "score", "prob_1"]
      }
    ]
  },
  "train": {
    "show_examples": false,
    "test_best": false,
    "validate_best": true,
    "batch_size": 16,
    "metrics": ["squad_f1", "exact_match", "squad_f1_1.1", "exact_match_1.1",
      "squad_roc_auc", "squad_pr", "squad_acc", "squad_score_f1"]
  },
  "metadata": {
    "labels": {
      "telegram_utils": "SquadModel",
      "server_utils": "SquadModel"
    }
  }
}

