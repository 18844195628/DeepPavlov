{
  "dataset_reader": {
    "name": "squad_dataset_reader",
    "dataset": "MultiSQuAD",
    "data_path": "multi_squad/"
  },
  "dataset_iterator": {
    "name": "multi_squad_shared_norm_iterator",
    "seed": 1337,
    "shuffle": true,
    "with_answer_rate": 0.666
  },
  "chainer": {
    "in": ["context_0_raw", "question_raw"],
    "in_y": ["ans_0_raw", "ans_0_raw_start", "context_1_raw", "ans_1_raw", "ans_1_raw_start"],
    "pipe": [
      {
        "name": "squad_preprocessor",
        "id": "squad_prepr",
        "context_limit": 450,
        "question_limit": 150,
        "char_limit": 16,
        "in": ["context_0_raw", "question_raw"],
        "out": ["context_0", "context_0_tokens", "context_0_chars",
                "c_0_r2p", "c_0_p2r", "question",
                "question_tokens", "question_chars", "spans_0"]
      },
      {
        "ref": "squad_prepr",
        "in": ["context_1_raw", "question_raw"],
        "out": ["context_1", "context_1_tokens", "context_1_chars",
                "c_1_r2p", "c_1_p2r", "question",
                "question_tokens", "question_chars", "spans_1"]
      },
      {
        "name": "squad_ans_preprocessor",
        "id": "squad_ans_prepr",
        "in": ["ans_0_raw", "ans_0_raw_start", "c_0_r2p", "spans_0"],
        "out": ["ans_0", "ans_0_start", "ans_0_end"]
      },
      {
        "ref": "squad_ans_prepr",
        "in": ["ans_1_raw", "ans_1_raw_start", "c_1_r2p", "spans_1"],
        "out": ["ans_1", "ans_1_start", "ans_1_end"]
      },
      {
        "name": "squad_vocab_embedder",
        "id": "vocab_embedder",
        "level": "token",
        "emb_folder": "embeddings/",
        "emb_url": "http://files.deeppavlov.ai/embeddings/wiki-news-300d-1M.vec",
        "save_path": "multi_squad_model_noans/emb/vocab_embedder.pckl",
        "load_path": "multi_squad_model_noans/emb/vocab_embedder.pckl",
        "context_limit": "#squad_prepr.context_limit",
        "question_limit": "#squad_prepr.question_limit",
        "char_limit": "#squad_prepr.char_limit",
        "fit_lol": ["context_0_tokens", "question_tokens"],
        "in": ["context_0_tokens", "question_tokens"],
        "out": ["context_0_tokens_idxs", "question_tokens_idxs"]
      },
      {
        "ref": "vocab_embedder",
        "in": ["context_1_tokens", "question_tokens"],
        "out": ["context_1_tokens_idxs", "question_tokens_idxs"]
      },
      {
        "name": "squad_vocab_embedder",
        "id": "char_vocab_embedder",
        "level": "char",
        "emb_folder": "embeddings/",
        "emb_url": "http://files.deeppavlov.ai/embeddings/wiki-news-300d-1M-char.vec",
        "save_path": "multi_squad_model_noans/emb/char_vocab_embedder.pckl",
        "load_path": "multi_squad_model_noans/emb/char_vocab_embedder.pckl",
        "context_limit": "#squad_prepr.context_limit",
        "question_limit": "#squad_prepr.question_limit",
        "char_limit": "#squad_prepr.char_limit",
        "fit_lol": ["context_0_chars", "question_chars"],
        "in": ["context_0_chars", "question_chars"],
        "out": ["context_0_chars_idxs", "question_chars_idxs"]
      },
      {
        "ref": "char_vocab_embedder",
        "in": ["context_1_chars", "question_chars"],
        "out": ["context_1_chars_idxs", "question_chars_idxs"]
      },
      {
        "name": "squad_model_shared_norm",
        "id": "squad",
        "word_emb": "#vocab_embedder.emb_mat",
        "char_emb": "#char_vocab_embedder.emb_mat",
        "context_limit": "#squad_prepr.context_limit",
        "question_limit": "#squad_prepr.question_limit",
        "char_limit": "#squad_prepr.char_limit",
        "train_char_emb": true,
        "char_hidden_size": 100,
        "encoder_hidden_size": 75,
        "attention_hidden_size": 75,
        "learning_rate": 0.5,
        "min_learning_rate": 0.001,
        "learning_rate_patience": 5,
        "keep_prob": 0.7,
        "grad_clip": 5.0,
        "weight_decay": 1.0,
        "save_path": "squad_model_shared_norm/model",
        "load_path": "squad_model_shared_norm/model",
        "in": {
          "c_left_tokens": "context_0_tokens_idxs",
          "c_left_chars": "context_0_chars_idxs",
          "q_tokens": "question_tokens_idxs",
          "q_chars": "question_chars_idxs"
        },
        "in_y": {
          "c_right_tokens": "context_1_tokens_idxs",
          "c_right_chars": "context_1_chars_idxs",
          "y1s_left": "ans_0_start",
          "y2s_left": "ans_0_end",
          "y1s_right": "ans_1_start",
          "y2s_right": "ans_1_end"
        },
        "out": ["ans_start_predicted", "ans_end_predicted", "prob", "logit"]
      },
      {
        "name": "squad_ans_postprocessor",
        "id": "squad_ans_postprepr",
        "in": ["ans_start_predicted", "ans_end_predicted", "context_0_raw", "c_0_p2r", "spans_0"],
        "out": ["ans_predicted", "ans_start_predicted", "ans_end_predicted"]
      }
    ],
    "out": ["ans_predicted", "ans_start_predicted", "logit"]
  },
  "train": {
    "show_examples": false,
    "test_best": false,
    "validate_best": true,
    "log_every_n_batches": 250,
    "val_every_n_epochs": 1,
    "batch_size": 50,
    "pytest_max_batches": 2,
    "validation_patience": 10,
    "metrics": ["squad_f1", "exact_match", "squad_f1_1.1", "exact_match_1.1",
      "squad_roc_auc", "squad_pr", "squad_acc", "squad_score_f1"],
    "tensorboard_log_dir": "squad_model_shared_norm/logs"
  },
  "metadata": {
    "requirements": [
      "../dp_requirements/tf-gpu.txt"
    ],
    "labels": {
      "telegram_utils": "SquadModel",
      "server_utils": "SquadModel"
    },
    "download": [
      "http://files.deeppavlov.ai/deeppavlov_data/squad_model_1.4_cpu_compatible.tar.gz",
      {
        "url": "http://files.deeppavlov.ai/embeddings/wiki-news-300d-1M.vec",
        "subdir": "embeddings"
      },
      {
        "url": "http://files.deeppavlov.ai/embeddings/wiki-news-300d-1M-char.vec",
        "subdir": "embeddings"
      }
      ]
  }
}

