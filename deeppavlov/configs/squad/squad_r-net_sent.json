{
  "dataset_reader": {
    "name": "squad_dataset_reader",
    "dataset": "SQuAD+Sent",
    "data_path": "squad_sent/"
  },
  "dataset_iterator": {
    "name": "sent_squad_iterator",
    "seed": 1337,
    "shuffle": true
  },
  "chainer": {
    "in": ["context_raw", "context", "context_tokens", "context_chars", "context_sentences",
           "c_sent_st_token_idxs", "c_sent_end_token_idxs", "question_raw", "question",
           "question_tokens", "question_chars", "spans", "c_r2p", "c_p2r"],
    "in_y": ["ans_raw", "ans_raw_start"],
    "pipe": [
      {
        "name": "squad_ans_preprocessor",
        "id": "squad_ans_prepr",
        "in": ["ans_raw", "ans_raw_start", "c_r2p", "spans"],
        "out": ["ans", "ans_start", "ans_end"]
      },
      {
        "name": "squad_vocab_embedder",
        "id": "vocab_embedder",
        "level": "token",
        "emb_folder": "embeddings/",
        "emb_url": "http://lnsigo.mipt.ru/export/embeddings/wiki-news-300d-1M.vec",
        "save_path": "squad_model/emb/vocab_embedder.pckl",
        "load_path": "squad_model/emb/vocab_embedder.pckl",
        "context_limit": 450,
        "question_limit": 150,
        "char_limit": 16,
        "fit_lol": ["context_tokens", "question_tokens"],
        "in": ["context_tokens", "question_tokens"],
        "out": ["context_tokens_idxs", "question_tokens_idxs"]
      },
      {
        "name": "squad_vocab_embedder",
        "id": "char_vocab_embedder",
        "level": "char",
        "emb_folder": "embeddings/",
        "emb_url": "http://lnsigo.mipt.ru/export/embeddings/wiki-news-300d-1M-char.vec",
        "save_path": "squad_model/emb/char_vocab_embedder.pckl",
        "load_path": "squad_model/emb/char_vocab_embedder.pckl",
        "context_limit": "#vocab_embedder.context_limit",
        "question_limit": "#vocab_embedder.question_limit",
        "char_limit": "#vocab_embedder.char_limit",
        "fit_lol": ["context_chars", "question_chars"],
        "in": ["context_chars", "question_chars"],
        "out": ["context_chars_idxs", "question_chars_idxs"]
      },
      {
        "name": "squad_model_ref",
        "id": "squad",
        "word_emb": "#vocab_embedder.emb_mat",
        "char_emb": "#char_vocab_embedder.emb_mat",
        "context_limit": "#vocab_embedder.context_limit",
        "question_limit": "#vocab_embedder.question_limit",
        "char_limit": "#vocab_embedder.char_limit",
        "train_char_emb": true,
        "char_hidden_size": 100,
        "encoder_hidden_size": 75,
        "attention_hidden_size": 75,
        "learning_rate": 0.5,
        "min_learning_rate": 0.001,
        "learning_rate_patience": 5,
        "keep_prob": 0.7,
        "grad_clip": 5.0,
        "weight_decay": 1.0,
        "use_birnn_after_coatt": true,
        "use_birnn_after_selfatt": true,
        "use_highway_after_coatt": false,
        "use_highway_after_selfatt": false,
        "concat_att_inputs": true,
        "concat_bigru_outputs": true,
        "use_gated_attention": true,
        "num_match_layers": 1,
        "num_encoder_layers": 3,
        "number_of_hops": 1,
        "USE": true,
        "save_path": "squad_model_r_net_sent/model",
        "load_path": "squad_model_r_net_sent/model",
        "in": {
          "c_tokens": "context_tokens_idxs",
          "c_chars": "context_chars_idxs",
          "q_tokens": "question_tokens_idxs",
          "q_chars": "question_chars_idxs",
          "c_str": "context_tokens",
          "q_str": "question_tokens",
          "c_sentences": "context_sentences",
          "c_sent_st_token_idxs": "c_sent_st_token_idxs",
          "c_sent_end_token_idxs": "c_sent_end_token_idxs",
          "q_sentence": "question"
        },
        "in_y": {
          "y1s": "ans_start",
          "y2s": "ans_end"
        },
        "out": ["ans_start_predicted", "ans_end_predicted", "prob", "logit"]
      },
      {
        "name": "squad_ans_postprocessor",
        "id": "squad_ans_postprepr",
        "in": ["ans_start_predicted", "ans_end_predicted", "context_raw", "c_p2r", "spans"],
        "out": ["ans_predicted", "ans_start_predicted", "ans_end_predicted"]
      }
    ],
    "out": ["ans_predicted", "ans_start_predicted", "prob", "logit"]
  },
  "train": {
    "show_examples": false,
    "test_best": false,
    "validate_best": true,
    "log_every_n_batches": 250,
    "val_every_n_epochs": 1,
    "batch_size": 50,
    "pytest_max_batches": 2,
    "validation_patience": 10,
    "metrics": ["squad_f1", "exact_match"],
    "tensorboard_log_dir": "squad_model_r_net_sent/logs"
  },
  "metadata": {
    "requirements": [
      "../dp_requirements/tf-gpu.txt"
    ],
    "labels": {
      "telegram_utils": "SquadModel",
      "server_utils": "SquadModel"
    },
    "download": [
      "http://TODO",
      {
        "url": "http://lnsigo.mipt.ru/export/embeddings/wiki-news-300d-1M.vec",
        "subdir": "embeddings"
      },
      {
        "url": "http://lnsigo.mipt.ru/export/embeddings/wiki-news-300d-1M-char.vec",
        "subdir": "embeddings"
      }
      ]
  }
}

